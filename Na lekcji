class Balwan:
    def __init__(self,_imie,_mana,_ilosckilli,_czas):
        self.imie = _imie
        self.mana = _mana
        self.ilosckilli = _ilosckilli
        self.czas = _czas

    def inf(self):
        print(self.imie)
        print(self.mana)
        print(self.ilosckilli)
        print(self.czas)

class Czolg:    
    def __init__(self,_imie,_kaliber,_ilosckilli,_cm):
        self.imie = _imie
        self.kaliber = _kaliber
        self.ilosckilli = _ilosckilli
        self.cm = _cm

    def inf(self):
        print(self.imie)
        print(self.kaliber)
        print(self.ilosckilli)
        print(self.cm)

class Laptop:
    def __init__(self,_imie,_iloscklawiszy,_iloscnacisku,_procesor):
        self.imie = _imie
        self.iloscklawiszy = _iloscklawiszy
        self.iloscnacisku = _iloscnacisku
        self.procesor = _procesor

    def inf(self):
        print(self.imie)
        print(self.iloscklawiszy)
        print(self.iloscnacisku)
        print(self.procesor)



czolg1 = Czolg("Olgierdos", "du≈ºy", 10, 100)
czolg1.inf()  

balwan1 = Balwan("Franek", 1000, 55, 3600000)
balwan1.inf() 

laptop1 = Laptop("Pan od Informatyki", "?", "kilka milionow", "Intel Core i 9...")
laptop1.inf()
