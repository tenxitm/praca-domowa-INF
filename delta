import math

while True:
    print("licz a | zatrzymaj b")
    inp = input().upper()
    if inp == "a":
        a = float(input())
        b = float(input())
        c = float(input())

        delta = b**2 - 4*a*c
        if delta > 0:
            x1 =(math.sqrt(delta))/(2*a) 
            x2 =(math.sqrt(delta))/(2*a) 
            print(f'Mam 2 pierwiastki x1 ={x1} = {x2}')
        elif delta == 0:
            x = -b/2*a
            print(f'x{x}')
        else:
            print("nie ma pierwiastk√≥w")

    elif inp == "b":
        break
    else:
        print("nie ma takiej komendy")
